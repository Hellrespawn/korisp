(
    set (a "a") (                 # Enter new scope with `a`
        (print a)

        (set ((b "b") (c "c")) (  # Enter new scope with `b` and `c`
            (print a)             # 'a' available from parent scope
            (print b)
            (print c)
        ))                        # Exit scope with `b` and `c`

        (print b)                 # Error `b` not defined

    )                             # Exit scope with `a`
)
